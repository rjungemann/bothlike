<!DOCTYPE html>
<html>
	<head>
	  <title>Yout Both Like</title>
	  <%= stylesheet_link_tag :all %>
	  <%= javascript_include_tag :defaults %>
		<script>
			function loaded(e) {
				if(window != window.top) {
					$(document.body).setStyle({
						backgroundColor: "#ffffff"
					});
					$("content").setStyle({
						borderLeft: "none",
						borderRight: "none"
					});
				}
			}
		</script>
	  <%= csrf_meta_tag %>
	</head>
	<body onload="loaded(event);">
		<div id="content">
			<%= yield %>
		</div>
	</body>
</html>
